{
	"name": "Wes Monkey",
	"version": "1.0",
	"manifest_version": 2,
	"description": "This extension makes the internet better.",
	"browser_action": {
		"default_icon": "icon.png"
	},
	 "background":{
	 	"scripts":["background.js"],
	 	"persistent": false
	 },
	 "permissions":[
	 	"background",
	 	"tabs",
	 	"http://*/*",
	 	"https://*/*"
	 ]
}
